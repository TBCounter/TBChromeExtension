{
    "name": "Count starter",
    "description": "Starts chests counting for TB counter",
    "version": "1.2",
    "manifest_version": 3,
    "icons": {
        "16": "icons/16.png",
        "32": "icons/32.png",
        "48": "icons/48.png",
        "128": "icons/128.png",
        "512": "icons/512.png"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "cookies",
        "storage"
    ],
    "host_permissions": [
        "https://totalbattle.com/",
        "*://*/*"
    ],
    "background": {},
    "author": "omega.interval.soft@gmail.com",
    "homepage_url": "http://totalbattle.omegasoft.keenetic.name/"
}